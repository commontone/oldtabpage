{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page, but not really",
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "chrome_url_overrides" : {
    "bookmarks": "second.html"
  },
  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}